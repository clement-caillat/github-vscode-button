{
    "name": "VSCode Extension GitHub",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "See every public git repositories with VSCode",
    "content_scripts": [
        {
            "matches": ["https://github.com/*"],
            "js": ["jquery-3.5.1.min.js", "fontawesome.js", "content.js"]
        }
    ],
    "icons": {
        "16": "images/icon-0.png",
        "32": "images/icon-1.png",
        "48": "images/icon-2.png",
        "128": "images/icon-3.png"
    }
}